{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-const-assign": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-else-if": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-empty-character-class": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-import-assign": 2,
    "no-irregular-whitespace": 2,
    "no-self-compare": 2,
    "no-template-curly-in-string": 1,
    "no-unexpected-multiline": 1,
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "camelcase": 2,
    "curly": 2,
    "default-case": 2,
    "default-case-last": 2,
    "eqeqeq": 2,
    "multiline-comment-style": 2,
    "max-classes-per-file": 2,
    "no-console": 1,
    "no-else-return": 2,
    "no-empty": 2,
    "no-var": 2,
    "prefer-const": 2,
    "prefer-destructuring": 2,
    "array-bracket-newline": [
      "error",
      {
        "multiline": true,
        "minItems": 2
      }
    ],
    "array-bracket-spacing": [
      "error",
      "always",
      {
        "arraysInArrays": false,
        "objectsInArrays": false,
        "singleValue": false
      }
    ],
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "comma-spacing": 2,
    "eol-last": 2,
    "indent": [
      "error",
      2
    ],
    "no-multi-spaces": 2,
    "jsx-quotes": 2
  }
}